{
  "name": "@instantdomain/bandit",
  "version": "0.1.0",
  "types": "index.d.ts",
  "main": "index.js",
  "exports": {
    ".": "./index.js",
    "./server": "./server.js",
    "./api": "./api.js"
  },
  "files": [
    "lib",
    "components",
    "api.js",
    "server.js",
    "pages/bandit-server.js",
    "pages/api/metrics.js",
    "pages/api/sites/[siteName].js",
    "docker-compose.dev.yml"
  ],
  "scripts": {
    "dc": "docker-compose -f docker-compose.dev.yml --env-file .env.local",
    "up": "yarn dc up -d",
    "rc": "yarn dc exec redis redis-cli",
    "rct": "yarn dc exec redis-testing redis-cli",
    "dev": "NODE_OPTIONS='--inspect=localhost:9229' next dev && yarn dc up -d",
    "build": "tsc -p tsconfig.publish.json",
    "start": "next start",
    "test": "jest --verbose",
    "clean": "rm -rf dist",
    "package": "yarn clean && yarn build && sh package.sh"
  },
  "dependencies": {
    "ioredis": "^5.0.6",
    "tslib": "^2.4.0"
  },
  "peerDependencies": {
    "react": "^18.1.0",
    "react-dom": "^18.1.0"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^5.16.4",
    "@testing-library/react": "^13.2.0",
    "@types/ioredis": "^4.28.10",
    "@types/jest": "^26.0.23",
    "@types/node": "^15.6.1",
    "@types/react": "^18.0.9",
    "babel-jest": "^27.0.2",
    "jest": "^27.0.3",
    "jest-fetch-mock": "^3.0.3",
    "jest-localstorage-mock": "^2.4.14",
    "next": "^12.1.6",
    "react": "^18.1.0",
    "react-dom": "^18.1.0",
    "typescript": "^4.3.2"
  }
}
